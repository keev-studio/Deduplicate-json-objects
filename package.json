{
  "name": "deduplicate-json-objects",
  "displayName": "Deduplicate Objects from JSON",
  "description": "Removes duplicate objects from JSON files based on a specified field",
  "version": "0.1.1",
  "publisher": "keev-studio",
  "author": {
      "name": "Keev Studio"
  },
  "engines": {
      "vscode": "^1.60.0"
  },
  "icon": "images/logo.png",
  "repository": {
      "type": "git",
      "url": "https://github.com/keev-studio/Deduplicate-json-objects"
  },
  "categories": [
      "Other"
  ],
  "keywords": [
      "json",
      "object",
      "duplicate",
      "deduplicate",
      "filter",
      "clean"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
      "commands": [
          {
              "command": "deduplicate-json-objects.removeDuplicates",
              "title": "Deduplicate Objects from JSON"
          }
      ],
      "menus": {
          "editor/context": [
              {
                  "when": "resourceExtname == .json",
                  "command": "deduplicate-json-objects.removeDuplicates",
                  "group": "1_modification"
              }
          ]
      }
  },
  "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "node ./test/runTest.js"
  },
  "devDependencies": {
      "@types/vscode": "^1.60.0",
      "@types/glob": "^7.1.3",
      "@types/node": "14.x",
      "eslint": "^7.27.0",
      "glob": "^7.1.7",
      "vscode-test": "^1.5.2"
  }
}